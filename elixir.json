{
  "app": {
    "title": "Starter Web App",
    "filename": "app",
    "mode": "development"
  },
  "sourcemaps": true,
  "publicPath": "public",
  "assetsPath": "app/assets",
  "viewPath": "app/views",
  "clean": {
    "sourceFolder": [
      ".tmp",
      ".temp",
      ".sass-cache",
      "./public"
    ],
    "read": false,
    "pluginOptions": {
      "force": true
    }
  },
  "server": {
    "proxy": false,
    "baseDir": "./public",
    "host": "http://localhost:3000"
  }
}
